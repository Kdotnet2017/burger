 <div class="col-sm-4" id="burgerDiv">
      {{#each burgers}}
      {{#unless devoured}}
    <ul>
            {{id}})-  {{burgers_name}}
            <button class="devouredClass btn btn-info" data-id={{id}} >Devoure it!</button>
    </ul>
    {{/unless}}
    {{/each}}
 </div>
<div class="col-sm-2"></div>
<div class="col-sm-4" id="devouredDiv">
 {{#each burgers}}
      {{#if devoured}}
    <ul>
            {{id}})-  {{burgers_name}}
    </ul>
    {{/if}}
    {{/each}}
</div>